@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdministratorLayout.cshtml";
}

<div class="jumbotron">
    <h2>Admin Panel</h2>
    <h4> Hello @HttpContext.Current.User.Identity.Name</h4>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Check all Rubrics</h2>
        @Html.ActionLink("Change Password", "ChangePassword", "Admin", new { @class = "btn btn-primary" })
    </div>
    <div class="col-md-4">
        <h2>Instructor List</h2>
        @Html.ActionLink("Click Here", "AllInstructors", "Instructor", new { @class = "btn btn-primary" })
    </div>
    <div class="col-md-4">
        <h2>Assessment Data</h2>
        @Html.ActionLink("Click Here", "AllData", "Result", new { @class = "btn btn-primary" })
    </div>
</div>

